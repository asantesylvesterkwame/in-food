{
  "expo": {
    "name": "in-food",
    "slug": "in-food",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/InFood.png",
    "userInterfaceStyle": "dark",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/InFood.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/InFood.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.asantesylvesterkwame.infood"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "106b08f8-93c1-459b-a797-b3b3987221cd"
      }
    }
  }
}
